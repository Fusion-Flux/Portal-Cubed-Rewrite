{
	"schema_version": 1,
	"quilt_loader": {
		"group": "io.github.fusionflux",
		"id": "portalcubed",
		"version": "${version}",
		"metadata": {
			"name": "Portal Cubed",
			"description": "A Portal mod for Quilt.",
			"contributors": {
        "Fusion Flux": "Owner",
        "Maximum": "Developer",
        "TropheusJ": "Developer",
        "Cart3r": "Artist"
			},
			"contact": {
				"issues": "https://github.com/Fusion-Flux/Portal-Cubed-Rewrite/issues",
				"sources": "https://github.com/Fusion-Flux/Portal-Cubed-Rewrite"
			},
			"icon": "assets/portalcubed/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": "io.github.fusionflux.portalcubed.PortalCubed",
      "client_init": "io.github.fusionflux.portalcubed.PortalCubedClient",
      "fabric-datagen": "io.github.fusionflux.portalcubed.PortalCubedDataGen"
    },
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=${loader_version}"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=${qfapi_version}"
			},
      {
        "id": "terraform-wood-api-v1",
        "versions": ">=${terraform_wood_api_version}"
      },
			{
				"id": "minecraft",
				"versions": ">=${minecraft_version}"
			}
		]
	},
	"mixin": "portalcubed.mixins.json",
  "access_widener": "portalcubed.accesswidener",

  "quilt_loom": {
    "injected_interfaces": {
      "net/minecraft/class_1657": [ "io/github/fusionflux/portalcubed/framework/extension/PlayerExt" ],
      "net/minecraft/class_1937": [ "io/github/fusionflux/portalcubed/framework/extension/LevelExt" ],
      "net/minecraft/class_3218": [ "io/github/fusionflux/portalcubed/framework/extension/ServerLevelExt" ],
      "net/minecraft/class_638": [ "io/github/fusionflux/portalcubed/framework/extension/ClientLevelExt" ]
    }
  }
}
