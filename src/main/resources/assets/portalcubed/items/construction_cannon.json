{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:context_entity_type",
    "cases": [
      {
        "when": "minecraft:fox",
        "model": {
 	   "type": "minecraft:model",
 	   "model": "portalcubed:item/construction_cannon_fox"
        }
      },
      {
        "when": [
          "minecraft:villager",
          "minecraft:witch",
          "minecraft:wandering_trader"
        ],
        "model": {
 	   "type": "minecraft:model",
 	   "model": "portalcubed:item/construction_cannon_villager"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "portalcubed:item/construction_cannon"
    }
  }
}